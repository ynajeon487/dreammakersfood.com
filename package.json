    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "esbuild": "^0.25.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "stylelint-config-standard-scss": "^16.0.0",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.5",
    "typescript": "5.6.3",
    "vite": "^5.4.20"
